{
  "name": "eddiehub",
  "version": "1.8.1",
  "description": "Discord bot built using NestJS",
  "author": "",
  "private": true,
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EddieHubCommunity/EddieBot.git"
  },
  "bugs": {
    "url": "https://github.com/EddieHubCommunity/EddieBot/issues"
  },
  "homepage": "https://github.com/EddieHubCommunity/EddieBot#readme",
  "scripts": {
    "prebuild": "rimraf prod",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r dotenv/config prod/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "if test \"$HUSKY\" != \"0\" ; then husky ; fi",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@discordjs/rest": "^2.3.0",
    "alex": "^11.0.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.4.4",
    "rimraf": "^5.0.7",
    "typo-js-ts": "^2.0.4",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
