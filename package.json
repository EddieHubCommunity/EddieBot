{
  "name": "eddiehub",
  "version": "1.7.8",
  "description": "Discord bot built using NestJS",
  "author": "",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EddieHubCommunity/EddieBot.git"
  },
  "bugs": {
    "url": "https://github.com/EddieHubCommunity/EddieBot/issues"
  },
  "homepage": "https://github.com/EddieHubCommunity/EddieBot#readme",
  "scripts": {
    "prebuild": "rimraf prod",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r dotenv/config prod/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "if test \"$HUSKY\" != \"0\" ; then husky install ; fi",
    "test": "jest"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "alex": "^9.1.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^6.13.0",
    "rimraf": "^3.0.2",
    "typo-js-ts": "^2.0.4",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^12.1.4",
    "@jest/globals": "^29.7.0",
    "@types/alex": "^9.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^6.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}